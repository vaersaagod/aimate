{
    "name": "vaersaagod/aimate",
    "description": "Ai ai, mate.",
    "type": "craft-plugin",
    "version": "1.0.0-alpha.1",
    "require": {
        "php": "^8.1.0",
        "craftcms/cms": "^4.4.0",
        "openai-php/client": "^v0.6.2"
    },
    "require-dev": {
        "craftcms/ecs": "dev-main",
        "craftcms/phpstan": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "vaersaagod\\aimate\\": "src/"
        }
    },
    "extra": {
        "handle": "_aimate",
        "name": "AIMate",
        "developer": "Værsågod",
        "documentationUrl": "",
        "class": "vaersaagod\\aimate\\AIMate"
    },
    "scripts": {
        "check-cs": "ecs check --ansi",
        "fix-cs": "ecs check --ansi --fix",
        "phpstan": "phpstan --memory-limit=1G"
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "8.1"
        },
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    }
}

